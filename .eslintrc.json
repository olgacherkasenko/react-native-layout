{
        "extends": [
          "plugin:react/recommended",
          "airbnb",
          "prettier",
          "prettier/react"
        ],
        "rules": {
          "react/jsx-filename-extension": [
            1,
            {
              "extensions": [
                ".js",
                ".jsx"
              ]
            }
          ],
          "prettier/prettier": [
            "error",
            {
              "trailingComma": "es5",
              "singleQuote": true,
              "printWidth": 100,
              "endOfLine": "auto"
            }
          ]
        },
        "plugins": [
          "react",
          "prettier"
        ]
}
